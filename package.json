{
  "name": "dock-components",
  "version": "1.8.4",
  "author": "dockmarket",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/main.js"
  },
  "info": "This is a proprietary package, any problem contact contact@dockmarket.com.br",
  "private": false,
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd && yarn publish",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd --file dist/index.umd.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --file dist/index.esm.js",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --file dist/index.min.js",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "test": "mochapack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js"
  },
  "files": [
    "dist/*",
    "src/*",
    "attributes.json",
    "tags.json"
  ],
  "dependencies": {
    "@storybook/addon-links": "^5.3.19",
    "@vue/cli-plugin-unit-jest": "^4.5.3",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "moment": "^2.27.0",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.11",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-material": "^1.0.0-beta-14",
    "vue-multiselect-fork": "^2.1.10",
    "vue-template-compiler": "^2.6.10",
    "vue-the-mask": "^0.11.1",
    "vuejs-datepicker": "^1.6.2"
  },
  "devDependencies": {
    "@storybook/vue": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/vue": "^5.0.4",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "jest-transform-stub": "^2.0.0",
    "minimist": "^1.2.0",
    "mocha": "^8.1.1",
    "mochapack": "^2.0.3",
    "node-sass": "^4.14.1",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
